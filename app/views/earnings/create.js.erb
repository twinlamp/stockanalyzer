$("[class^='earning-']").remove();
$('#earningstable tbody').prepend("<%= j render partial: 'earnings/earning', collection: @stock.earnings.reverse %>");
$(function () {
    $('#highchart').highcharts('StockChart', {
       chart: {
            alignTicks: false,
            height: 800,
            },
       yAxis: [{
            type: 'logarithmic',
            ceiling: '<%= @graph_max*20 %>',
            max: '<%= @graph_max*20 %>',
            min: '<%= @graph_min*20 %>',
            startOnTick: false,
            endOnTick: false,
            opposite: false
        } , {
            type: 'logarithmic',
            ceiling: '<%= @graph_max %>',
            max: '<%= @graph_max %>',
            min: '<%= @graph_min %>',
            startOnTick: false,
            endOnTick: false,
            opposite: true
        }],
        xAxis: {
            min: '<%= @stock.earnings[3].report.to_time.to_i*1000 %>'
        },
        series: [{
            name: "Price",
            data: <%= raw @prices  %>
        } , {
            name: "Earnings",
            data: <%= raw @stock.get_trailing_eps %>,
            yAxis: 1,
            type: 'column'
        }]
    });
});
