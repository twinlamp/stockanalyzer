$("[class^='earning-']").remove();
$('#earningstable tbody').prepend("<%= j render partial: 'earnings/earning', collection: @earning.stock.earnings.reverse %>");
$('#chart-data').data("graphmax", <%= graph_max(@earning.stock) %>);
$('#chart-data').data("graphmin", <%= graph_min(@earning.stock) %>);
$('#chart-data').data("earnings", <%= highchart_earnings(@earning.stock) %>).trigger('datachange');
