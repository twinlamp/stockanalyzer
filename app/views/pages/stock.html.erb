<%= line_chart [
  {name: "Price", data: @prices },
  {name: "Earnings", data: @earnings, yAxis: 1, type: 'column'}
],

library: {
		chart: {
			alignTicks: false
			},
        yAxis: [{
        	type: 'logarithmic',
        	ceiling: 200,
        	max: 200,
        	endOnTick: false,
            opposite: false
        }, {
        	type: 'logarithmic',
        	ceiling: 10,
        	max: 10,
        	endOnTick: false,
            opposite: true
        }]
    }
 %>