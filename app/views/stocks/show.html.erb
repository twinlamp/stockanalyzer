<div id='highchart'></div>
<script>
    $(function () {
        $('#highchart').highcharts('StockChart', {
           chart: {
                alignTicks: false,
                width: 1000,
                height: 800,
                },
           yAxis: [{
                type: 'logarithmic',
                ceiling: <%= @max*20 %>,
                max: <%= @max*20 %>,
                min: <%= @min*20 %>,
                startOnTick: false,
                endOnTick: false,
                opposite: false
            }, {
                type: 'logarithmic',
                ceiling: <%= @max %>,
                max: <%= @max %>,
                min: <%= @min %>,
                startOnTick: false,
                endOnTick: false,
                opposite: true
            }],
            series: [{
                name: "Price",
                data: <%= raw @prices %>
            }, {
                name: "Earnings",
                data: <%= raw @earnings %>,
                yAxis: 1,
                type: 'column'
            }]
        });
    });
</script>